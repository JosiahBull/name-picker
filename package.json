{
	"name": "name-picker",
	"version": "1.0.0",
	"private": true,
	"description": "A Tinder-style name selection app for couples",
	"type": "module",
	"scripts": {
		"setup": "./scripts/setup.sh",
		"build": "./scripts/build.sh",
		"test": "./scripts/test.sh",
		"console-report": "./scripts/console-report.sh",
		"lint": "pnpm -r run lint",
		"format": "pnpm -r run format && prettier --write \"tests/**/*.{json,js,ts,css,json}\"",
		"typecheck": "pnpm -r run typecheck",
		"clean": "pnpm -r run clean && rimraf node_modules",
		"supabase:start": "pnpm dlx supabase@latest start",
		"supabase:start:ci": "pnpm dlx supabase@latest start --exclude realtime,storage-api,imgproxy,studio,edge-runtime",
		"supabase:stop": "pnpm dlx supabase@latest stop --no-backup",
		"supabase:status": "pnpm dlx supabase@latest status",
		"supabase:reset": "pnpm dlx supabase@latest db reset",
		"gen-types": "pnpm dlx supabase@latest gen types typescript --local > packages/shared/src/database.types.ts"
	},
	"devDependencies": {
		"@eslint/js": "^9.32.0",
		"@playwright/test": "^1.54.1",
		"@supabase/auth-js": "^2.71.1",
		"@supabase/supabase-js": "^2.53.0",
		"@types/node": "^24.1.0",
		"@typescript-eslint/eslint-plugin": "^8.38.0",
		"@typescript-eslint/parser": "^8.38.0",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"prettier": "^3.6.2",
		"rimraf": "^6.0.1",
		"rollup-plugin-visualizer": "^6.0.3",
		"supabase": "^2.33.5",
		"typescript": "^5.8.3",
		"wrangler": "^4.26.1"
	},
	"engines": {
		"node": ">=22.0.0",
		"pnpm": ">=10.0.0"
	},
	"packageManager": "pnpm@10.13.1",
	"dependencies": {
		"@headlessui/react": "^2.2.6",
		"@preact/compat": "^18.3.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/postcss": "^4.1.11",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"autoprefixer": "^10.4.21",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"postcss": "^8.5.6",
		"preact": "^10.27.0",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11"
	}
}
